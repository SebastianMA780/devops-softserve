pipeline {
		agent any
		stages {
			stage('Test') {
				steps {
					echo 'Installing dependencies and running tests...'
				}
			}
		}
		post {
			success {
				build(job: 'Terraform-PR-Pipeline', propagate: true, parameters: [
                    string(name: 'PASSED_CHANGE_ID', value: env.CHANGE_ID ?: ''),
                    string(name: 'PASSED_CHANGE_URL', value: env.CHANGE_URL ?: '')
                ])
			}
		}
}
